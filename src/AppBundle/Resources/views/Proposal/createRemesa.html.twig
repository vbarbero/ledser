       });*/
   </script>

   <script type="text/javascript">
        calculaDias(0);
        var $collectionHolder;

        $(function() {
            $(".messageNotice").hide();

           $("#remesa_notice").on("click", function() {
		        if($("#remesa_notice").prop('checked') ) {
                    $(".messageNotice").show();
                } else {
                    $(".messageNotice").hide();
                }
            });

           $("#remesa_costeFinancieroTae").on("change", function() {
		        calculaNumeros();
            });

           if ($("#remesa_state").val() == 5) {
                $(".reason").show();
            } else {
                $(".reason").hide();
            }
            $("#remesa_state").on('change', function () {
                if ($("#remesa_state").val() == 5) {
                    $(".reason").show();
                } else {
                    $(".reason").hide();
                }
            });

            recalculaTotales() ; 
            /*$("#remesa_costeFinancieroTotal").on("change", function() {
                recalculaTablaCostes();
            });*/
            $collectionHolder = $('table.calculadora');
            $collectionHolder.data('index', $collectionHolder.find(':input').length);
            $(".add_calculator_link").on('click', function(e) {
               addCalculatorForm($collectionHolder);
            });
            var $wrapper = $('.js-calculadora-wrapper');
            $wrapper.on('click', '.js-remove-calculadora', function(e) {
               e.preventDefault();
               $(this).closest('.js-calculadora-item')
                   .remove();
            });

        });

       function addCalculatorForm($collectionHolder) {
           // Get the data-prototype explained earlier
           var prototype = $collectionHolder.data('prototype');
           var index = $collectionHolder.data('index');
           var newForm = prototype;
           newForm = newForm.replace(/__name__/g, index);
           $collectionHolder.data('index', index + 1);
           $collectionHolder.append(newForm);


           var d = new Date();
           $("#remesa_calculadora_" + index + "_formalizacion").val(d.getFullYear() + "-" + ("0" + (d.getMonth() + 1)).slice(-2) + "-" + ("0" + d.getDate()).slice(-2));
           $("#remesa_calculadora_" + index + "_vencimiento").val(d.getFullYear() + "-" + ("0" + (d.getMonth() + 1)).slice(-2) + "-" + ("0" + d.getDate()).slice(-2));
           recalculaTotales();
       }

       function recalculaTotales()
       {
           $(".linea-nominal").on("change", function() {
               $("#remesa_totalNominal").val(calculaTotalNominal());
            });
           $(".linea-dias").on("change", function() {
               $("#remesa_mediaDias").val(calculaMediaDias());
            });
           $(".linea-tae").on("change", function() {
               $("#remesa_mediaTae").val(calculaMediaTae());
            });
           $(".linea-costeFinanciero").on("change", function() {
               $("#remesa_totalCosteFinanciero").val(calculaTotalCosteFinanciero());
            });
           $(".linea-costeUnitario").on("change", function() {
               $("#remesa_totalCoste").val(calculaTotalCoste());
            });
           $(".linea-timbres").on("change", function() {
               $("#remesa_totalTimbres").val(calculaTotalTimbres());
            });
           calculaTimbreFila();

           $("input[id$='dias']").on("change", function() {
               var id = $(this).attr('id').split("_");
               reloadDates(id[2]);
               $("#remesa_mediaDias").val(calculaMediaDias());
            });
           $("input[id$='vencimiento']").on("change", function() {
               var id = $(this).attr('id').split("_");
               calculaDias(id[2]);
               $("#remesa_mediaDias").val(calculaMediaDias());
            });
           $("input[id$='formalizacion']").on("change", function() {
               var id = $(this).attr('id').split("_");
               calculaDias(id[2]);
               $("#remesa_mediaDias").val(calculaMediaDias());
            });
       }
       
       function calculaTotalNominal() {
            var total = 0;
            $(".linea-nominal").each(function( index ) {
                if($.isNumeric($(this).val())) {
                    total = parseFloat(total)+ parseFloat($(this).val());
                }
            });
            return total;
       }

       function calculaMediaDias() {
            var total = 0;
            $(".linea-dias").each(function( index ) {
                if($.isNumeric($(this).val())) {
                    total = parseFloat(total)+ parseFloat($(this).val());
                }
            });
            return total/$(".linea-dias").length;
       }

       function calculaMediaTae() {
           var total = 0;
            $(".linea-tae").each(function( index ) {
                if($.isNumeric($(this).val())) {
                    total = parseFloat(total)+ parseFloat($(this).val());
                }
            });
            return total/$(".linea-tae").length;
       }

       function calculaTotalCosteFinanciero() {
           var total = 0;
            $(".linea-costeFinanciero").each(function( index ) {
                if($.isNumeric($(this).val())) {
                    total = parseFloat(total)+ parseFloat($(this).val());
                }
            });
            return total;
       }

       function calculaTotalCoste() {
           var total = 0;
            $(".linea-costeUnitario").each(function( index ) {
                if($.isNumeric($(this).val())) {
                    total = parseFloat(total)+ parseFloat($(this).val());
                }
            });
            return total;
       }

       function calculaTotalTimbres() {
            var total = 0;
            $(".linea-timbres").each(function( index ) {
                if($.isNumeric($(this).val())) {
                    total = parseFloat(total)+ parseFloat($(this).val());
                }
            });
            return total;
       }
       
        function calculaDias(index) {

            console.log(index);
            var start = $("#remesa_calculadora_"+index+"_formalizacion").val();
            var startD = new Date(start);
            var end = $("#remesa_calculadora_"+index+"_vencimiento").val();
            var endD = new Date(end);
            var diff = parseInt((endD.getTime()-startD.getTime())/(24*3600*1000));
            $("#remesa_calculadora_"+index+"_dias").val(diff);
        }

        function  reloadDates(index) {
            console.log(index);
            var start = $("#remesa_calculadora_"+index+"_formalizacion").val();
            console.log(start);
            var startD = new Date(start);
            var finishD = new Date(start);
            var dateString = finishD.getUTCDate() +"/"+ (finishD.getUTCMonth()+1) +"/"+ finishD.getUTCFullYear() ;
            console.log(dateString);
            finishD.setDate(finishD.getDate() + parseInt($("#remesa_calculadora_"+index+"_dias").val()));
            var nd = new Date(finishD);
            var day = ("0" + nd.getDate()).slice(-2);
            var month = ("0" + (nd.getMonth() + 1)).slice(-2);
            var dateString = nd.getUTCFullYear() +"-"+ month +"-"+ day;
            console.log(dateString);
            $("#remesa_calculadora_"+index+"_vencimiento").val(dateString);
        }

        function timbre(valor)
        {
            varlor = parseFloat(valor);
            var stamp = 0.00;
            if(valor < 24.05) {
                stamp = 0.06;
            }else if(valor < 48.09) {
                stamp = 0.12;
            }else if(valor < 90.16) {
                stamp = 0.24;
            }else if(valor < 180.31) {
                stamp = 0.48;
            }else if(valor < 360.62) {
                stamp = 0.96;
            }else if(valor < 751.27) {
                stamp = 1.98;
            }else if(valor < 1502.54) {
                stamp = 4.21;
            }else if(valor < 3005.07) {
                stamp = 8.41;
            }else if(valor < 6010.13) {
                stamp = 16.83;
            }else if(valor < 12020.25) {
                stamp = 33.66;
            }else if(valor < 24040.49) {
                stamp = 67.31;
            }else if(valor < 48080.98) {
                stamp = 134.63;
            }else if(valor < 96161.95 ) {
                stamp = 269.25;
            }else if(valor < 192323.87 ) {
                stamp = 538.51;
            }
                console.log(stamp);
            return stamp;
        }
        
        function calculaTimbreFila() {
            $(".linea-existeTimbre").on('click', function() {
                if($(this).is(':checked'))
                {
                    var idNominal = $(this).attr('id').replace("existeTimbre", "nominal");
                    var idTimbre = $(this).attr('id').replace("existeTimbre", "timbres");
                    var valor = timbre($("#" + idNominal).val());
                    $("#" + idTimbre).val(valor);
                    $("#remesa_totalTimbres").val(calculaTotalTimbres());
                }
            });
        }

        function calculaCosteFinancieroTotal() {
           console.log("calculaCosteFinancieroTotal");
           console.log($("#remesa_totalCosteFinanciero").val());
           console.log($("#remesa_totalNominal").val());
           return parseFloat($("#remesa_totalCosteFinanciero").val()) / parseFloat($("#remesa_totalNominal").val()) * 100;
        }

        function calculaLiquido() {
           console.log($("#remesa_totalCosteFinanciero").val());
           console.log($("#remesa_totalNominal").val());
           return parseFloat($("#remesa_totalNominal").val()) - parseFloat($("#remesa_totalCosteFinanciero").val());
        }

        function getValue(index)
        {
            var aux = parseFloat($("#remesa_" + index).val());
            if(isNaN(aux))
                return 0;
            return aux;
        }

        function getValueFila(index,campo)
        {
            var aux = parseFloat($("#remesa_calculadora_" + index + "_" + campo).val());
            if(isNaN(aux))
                return 0;
            return aux;
        }

        function setValue(index, value)
        {
            return parseFloat($("#remesa_" + index).val(value));
        }

        function setValueFila(index,campo, value)
        {
            return parseFloat($("#remesa_calculadora_" + index + "_" + campo).val(value));
        }

        function calculaHonorariosFila(index)
        {
            return (getValueFila(index, 'costeFinanciero')/getValue('totalCosteFinanciero')) * getValue('honorarios');
        }

        function calculaCosteFinancieroLedserCosteFila(index)
        {
            return getValueFila(index, 'costeFinanciero') + calculaHonorariosFila(index);
        }

        function calculaCosteFinancieroLedserTaeFila(index)
        {
            return (360 * 100 * calculaCosteFinancieroLedserCosteFila(index)) / (getValueFila(index, 'nominal') * getValueFila(index, 'dias'));
        }

        function calculaCosteFinancieroledserTae()
        {
            var total = 0;
            var ids = calculaIds();
            var arrayLength = ids.length;
            for (var i = 0; i < arrayLength; i++) {
                total += calculaCosteFinancieroLedserTaeFila(ids[i]);
            }
            return total / arrayLength;
        }

        function calculaCosteTotalFila(index)
        {
            calculaCosteFinancieroLedserCosteFila(index) + getValueFila(index, 'timbres') +   (getValue('omf') +getValue('mensajeria') +getValue('burofax') +getValue('gastos'))/4;
        }

        function calculaCosteTotalTotalFila(index)
        {
            console.log('__calculaCosteTotalTotalFila__');
            console.log(calculaCosteTotalFila(index));
            console.log(getValueFila(index, 'nominal'));
            console.log(getValueFila(index, 'dias'));
            return (360 * 100 * calculaCosteTotalFila(index))/(getValueFila(index, 'nominal') * getValueFila(index, 'dias'));
        }

        function calculaCosteTotalTotal()
        {
            var total = 0;
            var ids = calculaIds();
            var arrayLength = ids.length;
                console.log('__calculaCosteTotalTotal__');
            for (var i = 0; i < arrayLength; i++) {
                console.log('calculaCosteTotalTotalFila');
                console.log(ids[i]);
                console.log(calculaCosteTotalTotalFila(ids[i]));
                total += calculaCosteTotalTotalFila(ids[i]);
            }
            return total / arrayLength;
        }




        function calculaTotalCosteFinancieroledserCoste()
        {
            var total = 0;
            var ids = calculaIds();
            var arrayLength = ids.length;
            for (var i = 0; i < arrayLength; i++) {
                total += calculaCosteFinancieroLedserCosteFila(ids[i]);
            }
            return total;
        }

        function calculaCosteFinancieroledserTotal()
        {
            return (calculaTotalCosteFinancieroledserCoste() / getValue('totalNominal')) * 100;
        }

        function calculaCosteFinancieroledserMensual()
        {
            return calculaCosteFinancieroledserTae() / 12;
        }

        function calculaTotalCosteFinancieroledserLiquido()
        {
            return getValue('totalNominal') - calculaTotalCosteFinancieroledserCoste();
        }

        function calculaTotalCosteTotalTotal()
        {
            return (calculaTotalCosteTotal() / getValue('totalNominal')) * 100;
        }


        function calculaTotalCosteTotal()
        {
            var total = 0;
            var ids = calculaIds();
            var arrayLength = ids.length;
            for (var i = 0; i < arrayLength; i++) {
                total += calculaCosteTotalFila(ids[i]);
            }
            return total;
        }


        function setTae()
        {
            var total = 0;
            var ids = calculaIds();
            var arrayLength = ids.length;
            console.log(ids);
            console.log("setTae");
            for (var i = 0; i < arrayLength; i++) {
                console.log("__" + i);
                setValueFila(ids[i], 'tae', getValue('costeFinancieroTae'));
                calculaCosteFinancieroFila(ids[i]);

            }
            $("#remesa_mediaTae").val(calculaMediaTae());
            $("#remesa_totalCosteFinanciero").val(calculaTotalCosteFinanciero());
        }

        function calculaCosteFinancieroFila(i) {
           console.log('---------');
           console.log((getValueFila(i, 'nominal') + getValueFila(i, 'dias') + getValueFila(i, 'tae'))/36000);
           console.log(getValueFila(i, 'nominal') );
           console.log(getValueFila(i, 'dias') );
           console.log(getValueFila(i, 'tae') );
           console.log('---------');
            setValueFila(i, 'costeFinanciero', (getValueFila(i, 'nominal') * getValueFila(i, 'dias') * getValueFila(i, 'tae'))/36000);
        }



        function calculaIds()
        {
            var valores = Array();
            $(".linea-nominal").each(function() {
                var ids = $(this).attr('id').split("_");
                valores.push(ids[2]);
            });
            return valores;
        }

        function calculaNumeros()
        {
            setTae();

